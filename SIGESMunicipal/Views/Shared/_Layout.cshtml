<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 
        Municipal</title>
   <!-- CSS only -->
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SIGEFMunicipal.styles.css" asp-append-version="true" />
</head>
<body class="text-white" >

    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
           
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/Resources/logo.png" style="height: 100px;margin-top: -20px;margin-bottom: -20px;"/></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                   
                </div>

            </div>
        </nav>
    </header>

    <input type="hidden" id="hdfOculto" value="@Url.Content("~")">
     <div class="bg-azul p-2 mb-4" style="margin-top: -19px;display: flex;justify-content: space-between;"><div class="">Ayuntamiento de algun lugar</div>  <div>Fulanodetal</div> </div>
    <div class="container-fluid ">
         <main role="main" class="pb-3 text-white" >
        <div class="row">
            <div class="col-12">
                <div class="row mb-2" style="text-align: right;padding: 0 25%;";>
                    <div class="col-3" onclick="location.href='@Url.Action("Index", "Home")'">
                    <button class="text-white font-weight-bold buttons-modules btn bg-azul ">Financiero</button>
                    </div>
                     <div class="col-3" onclick="location.href='@Url.Action("Index", "ComprasContrataciones")'">
                           <button class="text-white font-weight-bold buttons-modules btn bg-azul  ">Compras</button>
                    </div>
                     <div class="col-3" onclick="location.href='@Url.Action("Index", "RRHH")'">
                           <button class="text-white font-weight-bold buttons-modules btn bg-azul  ">RRHH</button>
                    </div>
                     <div class="col-3">
                           <button class="text-white font-weight-bold buttons-modules btn bg-azul  ">Otros</button>
                    </div>
                 
       
                </div>
        
            </div>
         
 
            <div class="col-12 p-4 border text-black" style="height:100vh">
                
                     @RenderBody()
               
            </div>
         
        </div>
         </main>
    </div>

    @*<footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - SIGEFMunicipal - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>*@
      <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/js/jquery.js"></script>
<script src="~/js/datatable.js"></script>
<script src="~/js/sweetalert.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
